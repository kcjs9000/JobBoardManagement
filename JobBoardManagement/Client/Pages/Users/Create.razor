@page "/user/create"
@inject HttpClient _client
@inject NavigationManager _navManager


<h3>Register User</h3>
<hr />
<br />

<FormComponent ButtonText="Register Resume" OnValidSubmit="@Createuser" user="user"/>

    @code {
        User user= new User();


       private async Task Createuser()
        {
            await _client.PostAsJsonAsync(Endpoint.UsersEndPoint, user);
            _navManager.NavigateTo("/user/");
        }

    }

