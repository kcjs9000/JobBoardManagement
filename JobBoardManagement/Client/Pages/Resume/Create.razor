@page "/resume/create"
@inject HttpClient _client
@inject NavigationManager _navManager


<h3>Register Resume</h3>
<hr />
<br />

<FormComponent ButtonText="Register Resume" OnValidSubmit="@Createresume" resume="resume" />

    @code {
        Resume resume= new Resume();


       private async Task Createresume()
        {
            await _client.PostAsJsonAsync(Endpoint.ResumeEndPoint, resume);
            _navManager.NavigateTo("/resume/");
        }

    }

